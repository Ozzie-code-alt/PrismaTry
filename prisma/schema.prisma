// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id   Int    @id @default(autoincrement())
  name String
  email String
  isAdmin Boolean 
  preferences Json
  blob Bytes 
}

model Post{
  rating Decimal
  createdAt DateTime
  updatedAt DateTime
}
